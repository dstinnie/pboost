<p align="center">
<table width="800" border="0" cellspacing="0" cellpadding="10" align="center">
	<tr>
		<td><%=link_to 'Add New Federal Campaign', new_federal_campaign_path %></td>
	<% if @federal_campaign && !@federal_campaign.new_record? %>
		<td><%= link_to 'View Federal Campaign', @federal_campaign %></td>
		<td><%= link_to 'Edit Federal Campaign', edit_federal_campaign_path(@federal_campaign) %></td>
		<td><%= link_to 'Delete Federal Campaign', @federal_campaign, :confirm => 'Are you sure?', :method => :delete %></td>
	</tr>
  <% end %>
</table>
</p>
