<p align="center">
<table width="800" border="0" cellspacing="0" cellpadding="10" align="center">
	<tr>
	<% if @gis_regions %>
		<td><%=link_to 'Add New GIS Region', new_gis_region_path %></td>	
	<% end %>
	<% if @gis_region %>
		<td><%= link_to 'List GIS Regions', gis_regions_path %></td>
		<% if !@gis_region.new_record? %>
			<td><%= link_to 'View GIS Region', @gis_region %></td>
			<td><%= link_to 'Edit GIS Region', edit_gis_region_path(@gis_region) %></td>
			<td><%= link_to 'Delete GIS Region', @gis_region, :confirm => 'Are you sure?', :method => :delete %></td>
		<% end %>
	</tr>
  <% end %>
</table>
</p>
