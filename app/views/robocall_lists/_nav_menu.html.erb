<p align="center">
<table width="100%" border="0" cellspacing="0" cellpadding="10" align="center">
	<tr>
	<% if @robocall_lists %>
		<td><%=link_to 'Create Robocall List', new_robocall_list_path %></td>
		<td><%=link_to 'Refresh Robocall Lists', robocall_lists_path %></td>
	<% end %>	
	<% if @robocall_list && !@robocall_list.new_record? %>
		<td><%=link_to 'List Robocall Lists', robocall_lists_path %></td>
		<td><%= link_to 'View Robocall List', @robocall_list %></td>
		<td><%= link_to 'Delete Robocall List', @robocall_list, :confirm => 'Are you sure you want to permanently delete this Robocall List?', :method => :delete %></td>
		<% if @robocall_list.upload_list == true && @robocall_list.need_mapping == true %>
			<td><%=link_to 'Finish Importing', map_fields_robocall_list_path(@robocall_list) %></td>
		<% end %>
	</tr>
  <% end %>
</table>
</p>
