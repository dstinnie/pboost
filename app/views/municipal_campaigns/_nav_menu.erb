<p align="center">
<table width="800" border="0" cellspacing="0" cellpadding="10" align="center">
	<tr>
		<td><%=link_to 'Add New Municipal Campaign', new_municipal_campaign_path %></td>
	<% if @municipal_campaign && !@municipal_campaign.new_record? %>
		<td><%= link_to 'View Municipal Campaign', @municipal_campaign %></td>
		<td><%= link_to 'Edit Municipal Campaign', edit_municipal_campaign_path(@municipal_campaign) %></td>
		<td><%= link_to 'Delete Municipal Campaign', @municipal_campaign, :confirm => 'Are you sure?', :method => :delete %></td>
	</tr>
  <% end %>
</table>
</p>
