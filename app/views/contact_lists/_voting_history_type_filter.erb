<% form.fields_for :voting_history_type_filter, voting_history_type_filter do |f| %>
	<p>Include voters who did <%= f.select :string_val, VotingHistoryTypeFilter::VOTING_HISTORY_TYPES -%> 
	<% if record.new_record? || record.voting_history_type_filter.nil? ||  ['Any','All'].include?(record.voting_history_type_filter.string_val) %>
		<span id="xval" style="display: none;">	
	<% else %>
		<span id="xval" style="display: inline;">	
	<% end %>
	<%= f.select :int_val, (1..Election.count-1).map -%></span>
of the following:

</p>
<% end %>

